{
  "name": "d-render-projects",
  "version": "1.0.0",
  "type": "module",
  "description": "d-render系列仓库",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev-styles": "gulp --require @esbuild-kit/cjs-loader -f ./build/tasks/styles/gulpfile.dev.js",
    "build-styles": "gulp --require @esbuild-kit/cjs-loader -f ./build/tasks/styles/gulpfile.js",
    "dev-js": "rollup -c ./build/tasks/javascript.js --watch",
    "build-js": "rollup -c ./build/tasks/javascript.js",
    "build-types": "node ./build/tasks/types.js",
    "build-typescript": "node ./build/tasks/typescript.js",
    "dev:d-render": "cross-env ENTRY_MODULE=d-render pnpm run dev-js",
    "build:d-render": "cross-env ENTRY_MODULE=d-render pnpm run '/^(build-js|build-styles|build-typescript)$/'",
    "dev:shared": "cross-env ENTRY_MODULE=shared pnpm run dev-js",
    "build:shared": "cross-env ENTRY_MODULE=shared pnpm run '/^(build-js|build-typescript)$/'",
    "dev:plugin-standard:core": "cross-env ENTRY_MODULE=plugin-standard/core pnpm run '/^(dev-js|dev-styles)$/'",
    "build:plugin-standard:core": "cross-env ENTRY_MODULE=plugin-standard/core pnpm run '/^(build-js|build-styles)$/'",
    "dev:plugin-standard:configure": "cross-env ENTRY_MODULE=plugin-standard/configure pnpm run '/^(dev-js|dev-styles)$/'",
    "build:plugin-standard:configure": "cross-env ENTRY_MODULE=plugin-standard/configure pnpm run '/^(build-js)$/'",
    "dev:design": "cross-env ENTRY_MODULE=design pnpm run '/^(dev-js|dev-styles)$/'",
    "build:design": "cross-env ENTRY_MODULE=design pnpm run '/^(build-js|build-styles)$/'",
    "lint": "eslint --fix build/**",
    "demo": "pnpm -C play serve",
    "changeset": "changeset",
    "gen:version": "changeset version",
    "build-all": "pnpm run '/^build:(.*)/'",
    "deploy:docs": "pnpm -C play build && scp -r play/dist/* x-develop.cn:~/web_html/docs/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@d-render/plugin-standard": "workspace:*",
    "@d-render/shared": "workspace:*",
    "d-render": "workspace:*",
    "play": "workspace:*"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@esbuild-kit/cjs-loader": "^2.4.2",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-standard": "^8.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "chalk": "^5.2.0",
    "consola": "^3.1.0",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.2.0",
    "fast-glob": "^3.2.12",
    "fglob": "^0.0.1",
    "fs-extra": "^11.1.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.7",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "vite": "^4.3.5",
    "vue": "^3.2.47"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
